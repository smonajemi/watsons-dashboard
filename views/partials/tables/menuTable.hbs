    <div class="modal fade" id="menuModals" tabindex="-1" role="dialog" aria-labelledby="menuModalsLabel"
      aria-hidden="true">
      <div class="modal-dialog">
        <div class="modal-content"></div>
      </div>
      <div class="modal-dialog">
        <div class="modal-content"></div>
      </div>
      <div class="modal-dialog">
        <div class="modal-content">
          <div class="modal-header">
            <h4 class="modal-title" id="menuModalsLabel">Update Menu Items</h4>
            <button type="button" class="close" data-dismiss="modal"> <span aria-hidden="true" class="">Ã—
              </span><span class="sr-only">Close</span>
            </button>
          </div>

          <div class="modal-body" id="modalBody">
            <form class="needs-validation" name="modalForm" action="/qrMenu" method="post" id="menuModalForm"
              novalidate>
              <div class="form-group" id="selectOptions">
                <label for="name" class="col-form-label">Type</label>
                <div class="form-row" id="validationCustom04">
                  <div class="col">
                    <select class="form-control selectpicker" name="type" title="Select type" required id="type">
                      <option selected disabled value="">Select</option>
                      <div id="selectBeerOptions">
                        {{#each beerMenuOptions}}
                        <option value="{{this}}">{{this}}</option>
                        {{/each}}
                      </div>
                      <div id="selectQrOptions">
                        {{#each qrOptions}}
                        <option value="{{this}}">{{this}}</option>
                        {{/each}}
                      </div>
                    </select>
                  </div>
                  <div class="col">
                    <div class="input-group mb-3">
                      <div class="input-group-prepend">
                        <div class="input-group-text">
                          <input type="checkbox" aria-label="Checkbox for following text input" id="typeCheckBox"
                            onclick="onDisabled()">
                        </div>
                      </div>
                      <input type="text" class="form-control" aria-label="Text input with checkbox" id="typeInput"
                        disabled="true" name="type">
                    </div>
                  </div>
                </div>
              </div>
              <div class="form-group">
                <label for="name" class="col-form-label">Name</label>
                <input type="text" name="name" id="name" class="form-control" required>
              </div>
              <div class="form-group">
                <label for="price" class="col-form-label">Price</label>
                <input type="text" step="0.01" name="price" id="price" class="form-control" required>
              </div>
              <div class="form-group">
                <label for="description" class="col-form-label">Description</label>
                <input type="text" name="description" id="description" class="form-control">
              </div>
              <div class="form-group" style="display: none;">
                <label for="description" class="col-form-label">Id</label>
                <input type="text" name="_id" id="id" class="form-control" readonly="readonly">
              </div>

              <div class="modal-footer actions">
                <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
                <button type="submit" class="btn btn-primary" id="saveModal" disabled="false"
                  onclick="onSaveModalItem();">Save changes</button>
              </div>
            </form>
          </div>

        </div>
